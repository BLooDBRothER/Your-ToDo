generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model users {
  id String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email String @unique
  name String
  profilePic String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
